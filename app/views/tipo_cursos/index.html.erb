<p id="notice"><%= notice %></p>

<h1>Tipo Cursos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_cursos.each do |tipo_curso| %>
      <tr>
        <td><%= tipo_curso.nome %></td>
        <td><%= link_to 'Show', tipo_curso %></td>
        <td><%= link_to 'Edit', edit_tipo_curso_path(tipo_curso) %></td>
        <td><%= link_to 'Destroy', tipo_curso, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Curso', new_tipo_curso_path %>
